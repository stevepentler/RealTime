<!doctype html>
<html>
  <head>
    <title>Real Time Voting</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.2/css/materialize.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body class="center">
    <h3>Survey Created!</h3>
    <h5 id="connection-count"></h5>
    <h5>Question: <%= survey.question %></h5>
    <h3><a href='<%= `/${survey.adminId}/${survey.id}`%>'>Admin Link</a></h3>
    <h3><a href='<%= `/survey/results/${survey.id}` %>'>Subscriber Link: Public Results</a></h3>
    <h3><a href='<%= `/survey/hideresults/${survey.id}` %>'>Subscriber Link: Private Results</a></h3>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.1/jquery.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.2/js/materialize.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/client.js"></script>
  </body>
</html>